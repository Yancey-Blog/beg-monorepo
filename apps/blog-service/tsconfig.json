{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "incremental": true,
    "removeComments": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "apollo-cache-control": ["node_modules/apollo-cache-control"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
