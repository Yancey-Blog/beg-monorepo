    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "helmet": "^6.0.0",
    "joi": "^17.4.2",
    "keycloak-connect": "^18.0.2",
    "luxon": "^3.0.3",
    "nest-keycloak-connect": "^1.7.6",
    "nest-winston": "^1.6.1",
    "reflect-metadata": "^0.1.13",
